generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Article {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  title       String @default("New Title")
  description String @default("New Description")
  conclusion  String @default("New Conclusion")
  img         String @default("")
  video       String @default("")
  body        String @default("This is my new body")
}
model Projects {
    id          String @id @default(auto()) @map("_id") @db.ObjectId
    slug        String @unique
    title       String @default("New Title")
    description String @default("New Description")
}